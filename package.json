{
  "name": "gptyped",
  "version": "1.0.5-alpha.10",
  "description": "Utility for making structured requests and get type safe responses from LLMs. Like using JSON for OpenAI GPT.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "ts-types": "tsc",
    "build": "rimraf dist && env NODE_ENV=production tsup",
    "dev": "rimraf dist && tsup --watch",
    "build:watch": "tsup --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p ./tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^18.15.12",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "zod": "3.21.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanignaciomolina/GPTyped.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "GPT",
    "OpenAi",
    "JSON",
    "ChatGPT",
    "Types"
  ],
  "author": "@juanignaciomolina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanignaciomolina/GPTyped/issues"
  },
  "homepage": "https://github.com/juanignaciomolina/GPTyped#readme"
}
