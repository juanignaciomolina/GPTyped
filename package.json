{
  "name": "gptyped",
  "version": "1.0.4-alpha.1",
  "description": "Utility for making structured requests and get type safe responses from LLMs. Like using JSON for OpenAI GPT.",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p ./configs/tsconfig.base.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanignaciomolina/GPTyped.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "GPT",
    "OpenAi",
    "JSON",
    "ChatGPT",
    "Types"
  ],
  "author": "@juanignaciomolina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanignaciomolina/GPTyped/issues"
  },
  "homepage": "https://github.com/juanignaciomolina/GPTyped#readme",
  "devDependencies": {
    "prettier": "^2.8.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "zod": "3.21.4"
  }
}
