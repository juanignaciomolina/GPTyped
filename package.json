{
  "name": "gptyped",
  "version": "1.0.0",
  "description": "Utility for making structured requests and get type safe responses from LLMs. Like using JSON for OpenAI GPT.",
  "main": "lib/index.ts",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanignaciomolina/GPTyped.git"
  },
  "keywords": [
    "GPT",
    "OpenAi",
    "JSON",
    "ChatGPT",
    "Types"
  ],
  "author": "@juanignaciomolina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juanignaciomolina/GPTyped/issues"
  },
  "homepage": "https://github.com/juanignaciomolina/GPTyped#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "prettier": "^2.8.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "openai": "^3.2.1",
    "zod": "^3.21.4"
  }
}
